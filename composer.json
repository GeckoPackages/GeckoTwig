{
    "name": "gecko-packages/gecko-twig",
    "type": "library",
    "homepage": "https://github.com/GeckoPackages",
    "description": "Additional Twig filters and tests.",
    "license": "MIT",
    "keywords": ["Twig", "Filter", "Bytes", "SI", "Roman"],
    "require": {
        "php": "^5.4.0 || >=7.0 <7.2",
        "twig/twig": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.5|^5",
        "symfony/phpunit-bridge": "^2.8|^3.0"
    },
    "suggest": {
        "ext-mbstring": "For multi byte character support."
    },
    "autoload": {
        "psr-4": {
            "GeckoPackages\\Twig\\": "src\\Twig"
        }
    }
}
